{
  "name": "tsops",
  "version": "1.2.2",
  "description": "TypeScript-first toolkit for planning, building, and deploying to Kubernetes",
  "type": "module",
  "bin": {
    "tsops": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b .",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist",
    "lint": "pnpm -w exec eslint .",
    "prepare": "tsc -b ."
  },
  "dependencies": {
    "@tsops/core": "workspace:*",
    "commander": "^14.0.1"
  },
  "keywords": [
    "kubernetes",
    "k8s",
    "devops",
    "deployment",
    "docker",
    "typescript",
    "ci-cd",
    "infrastructure"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pom4H/tsops.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/Pom4H/tsops#readme",
  "bugs": {
    "url": "https://github.com/Pom4H/tsops/issues"
  },
  "license": "MIT",
  "author": "Roman Popov",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
